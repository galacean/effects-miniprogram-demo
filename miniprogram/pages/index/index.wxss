.page {
  height: 100vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: top;
}

.page .page-section {
  width: 100%;
}

.page navigator {
  width: 100%;
  background-color: white;
  margin-top: 2rpx;
  line-height: 120rpx;
  text-align: center;
  border-bottom: 1rpx solid #eee;
}

.page navigator .navigator-hover {
  background-color: rgba(0, 0, 0, 0.1);
  color: #fff;
}
